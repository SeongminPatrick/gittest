install.packages("dplyr")
library(dplyr)
data(iris)
head(iris)
subset(iris$Sepal.Lenth, Species, mean)
subset(iris$Sepal.Length, Species, mean)
subset(iris$Sepal.Length, iris$Species, mean)
subset(iris$Sepal.Length, iris$Species, mean)
tapply(iris$Sepal.Length, iris$Species, mean)
subset(iris, Species == "setosa" & Sepal.Length > 4)
subset(iris, Species == "setosa" & Sepal.Length > 5)
subset(iris, Species == "setosa" & Sepal.Length > 6)
subset(iris, Species == "setosa" & Sepal.Length > 5.5)
